struct Person {
    name,
    email
}

Person.new = fn (name, email) {
    return Person { name, email }
}

create person = Person.new("Lugli", "lugli@test.com")

Console.println!(person.name)

struct Nested {
    person
}

create nested = Nested {
    person: Person {
        name: "Lugli",
        email: "lugli@test.com",
    }
}

Console.println!(nested.person.name)

nested.person.name = "Cleiton"

Console.println!(nested.person.name)