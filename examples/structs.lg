struct Person {
    name,
    email
}

Person.new = fn (name, email) {
    return Person { name, email }
}

let person = Person.new("Lugli", "lugli@test.com")

println!(person.name)

struct Nested {
    person
}

let nested = Nested {
    person: Person {
        name: "Lugli",
        email: "lugli@test.com",
    }
}

println!(nested.person.name)

nested.person.name = "Cleiton"

println!(nested.person.name)