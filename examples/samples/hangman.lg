struct Objetive {
	word
	founded = []

	fn new(word) {
		create obj = Objetive { word: word }

		for item in obj.word {
			obj.founded.push!(item)
		}

		Console.println!(obj.founded)

		return obj
	}

	fn check_letter(this, letter){
		if this.word.contains?(letter) {
			this.founded.push!(letter)
			return true
		}
		return false
	}

