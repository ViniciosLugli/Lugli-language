let empty = []

println(empty.isEmpty())

let nums = [1, 2, 3, 4, 5]

println(nums.reverse())
println(nums.join(", "))

println(nums.filter(fn (num) {
    return num < 3
}))

nums.each(fn (num) {
    println(num)
})

nums.each(println)

let incrementedNums = nums.map(fn (num) {
    return num + 1
})

println(incrementedNums)
println(incrementedNums.first())
println(incrementedNums.first(fn (num) {
    return num > 3
}))
