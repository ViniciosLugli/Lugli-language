create names = ["Lugli", "Gabs", "Edu Barreto", "Lucas gay", "Teugo", "Julho", "Zap Zap", "Pucca", "Campos"]

for name in names {
    println!(name)
}

for (i, name) in names {
    println!(i + ". " + name)
}

println!("-- breaking in first name --")

for name in names {
    println!(name)
    break
}

println!("-- continue skipping index 2 and 4 --")
for (i, name) in names {
    if i == 2 || i == 4{
        continue
    }

    println!(i + ". " + name)
}