create name = "Gabs"

println!(name.contains?("Ga"))
println!(name.startsWith?("Ga"))
println!(name.endsWith?("bs"))
println!(name.finish!(" gay"))
println!(name.append!(" gay"))

name.tap(fn (raw) {
    println!("Tapped value is: " + raw)
})

println!("this should be in uppercase".upper!())
println!("this should be in lowercase".lower!())